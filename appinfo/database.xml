				<default></default>
				<notnull>false</notnull>
				<length>255</length>
			</field>
			<field>
				<name>last_seen</name>
				<type>integer</type>
				<default></default>
				<notnull>true</notnull>
				<length>4</length>
			</field>
			<index>
				<name>saml_uid_pk</name>
